
import java.util.Scanner;

/**
 * GameOfLife
 * 
 * @author ethanzhou
 *April 24 2017
 */
public class Life {
	static Scanner br = new Scanner(System.in);
	static int[][] field = new int[10][10];
	static int i;
	static int n;
	
	
	public static void main(String[] args){
		if (n == 0){	
			for(i = 0; i < 10; i++){
				for(n = 0; n < 10; n++){
					System.out.print(field[i][n] = 0);	
				}
				System.out.println();
			}
		}else{
			start();
			board();
		}
		
	}
	
	
	public static int start(){
		System.out.println("please input coordiantes from 0 - 10");
		int ch1 = br.nextInt(); int ch2 = br.nextInt();
		field[ch1][ch2] = 1;
		return field[i][n];
	}
	
	
	public static void board(){
		for(i = 0; i < 10; i++){
			for(n = 0; n < 10; n++){
					System.out.print(field[i][n]);
			}
			System.out.println();
		}
	}
	
}
